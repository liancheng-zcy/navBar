.lac-nav-home-wrap {
  width: 100%;
  height: 32px;
  padding-left: 9px;
  text-align: left;
}
.lac-nav-home-wrap .slogn {
  height: 16px;
  font-size: 16px;
  color: #1F1F1F;
  line-height: 16px;
  font-weight: 600;
  display: flex;
  align-items: center;
}
.lac-nav-home-wrap .slogn .arrow-right {
  width: 7px;
  height: 7px;
  border-top: 1.5px solid #1F1F1F;
  border-right: 1.5px solid #1F1F1F;
  transform: rotate(45deg);
}
.lac-nav-home-wrap .tip {
  height: 10px;
  font-size: 10px;
  color: #C2C2C2;
  line-height: 10px;
  margin-top: 4px;
}